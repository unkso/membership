<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com">
  <import>
    
    <userhistorytype identifier="unkso.join">
      <name>Joined the Clan</name>
      <template><![CDATA[{$_username} joined the clan.]]></template>
      <class>wcf\data\membership\userhistory\JoinHistoryType</class>
    </userhistorytype>
    
    <userhistorytype identifier="unkso.promotion">
      <name>Promotion</name>
      <template><![CDATA[{$_username} has been promoted from {$oldRank} ({$oldPaygrade}) to {$newRank} ({$newPaygrade}).]]></template>
      <class>wcf\data\membership\userhistory\PromotionHistoryType</class>
    </userhistorytype>
    
    <userhistorytype identifier="unkso.demotion">
      <name>Demotion</name>
      <template><![CDATA[{$_username} has been demoted from {$oldRank} ({$oldPaygrade}) to {$newRank} ({$newPaygrade}).]]></template>
      <class>wcf\data\membership\userhistory\DemotionHistoryType</class>
    </userhistorytype>
    
    <userhistorytype identifier="unkso.award">
      <name>Award</name>
      <template><![CDATA[{$_rank}.{$_username} has received the {$award} for this reason: {$reason}]]></template>
      <class>wcf\data\membership\userhistory\AwardReceivedHistoryType</class>
    </userhistorytype>
    
    <userhistorytype identifier="unkso.transfer">
      <name>Transfer</name>
      <template><![CDATA[{$_rank}.{$_username} has been transferred from {$oldUnit} to {$newUnit}.]]></template>
      <class>wcf\data\membership\userhistory\TransferHistoryType</class>
    </userhistorytype>
      
  </import>
</data>